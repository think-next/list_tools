{
  "manifest_version": 3,
  "name": "List Tools",
  "version": "0.1.0",
  "description": "本地工具集合，提效工作",
  "author": "List Tools Team",
  "permissions": [
    "tabs"
  ],
  "action": {
    "default_title": "List Tools"
  },
  "background": {
